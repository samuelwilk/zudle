<turbo-stream action="replace" targets="#game_{{ gameState.game.id }}">
    <template>
        <twig:GameBoard gameState="{{ gameState }}" />
    </template>
</turbo-stream>

<turbo-stream action="update" targets="#keyboard">
    <template>
        {{ include('game/_game_keyboard.html.twig', { gameState: gameState }) }}
    </template>
</turbo-stream>
